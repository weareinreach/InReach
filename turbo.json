{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"env": ["NODE_ENV"]
		},
		"@weareinreach/app#build": {
			"dependsOn": ["^build"],
			"env": [
				"DATABASE_URL",
				"NEXTAUTH_SECRET",
				"NEXTAUTH_URL",
				"VERCEL_URL",
				"PORT",
				"COGNITO_CLIENT_ID",
				"COGNITO_CLIENT_SECRET",
				"COGNITO_ISSUER",
				"COGNITO_USER_POOL_ID",
				"AWS_REGION"
			],
			"outputs": [".next/**"]
		},
		"@weareinreach/storybook#build": {
			"dependsOn": ["^build"],
			"outputs": ["storybook-static/**", ".next/**"]
		},
		"@weareinreach/web#build": {
			"dependsOn": ["^build"],
			"env": [
				"SENTRY_DSN",
				"SANITY_TOKEN",
				"NEXT_PUBLIC_SANITY_API_VERSION",
				"NEXT_PUBLIC_SANITY_DATASET",
				"NEXT_PUBLIC_SANITY_PROJECT_ID",
				"NEXT_PUBLIC_SENTRY_DSN"
			],
			"outputs": [".next/**"]
		},
		"chromatic": {
			"dependsOn": ["build"],
			"env": ["CHROMATIC_PROJECT_TOKEN"]
		},
		"dev": {
			"cache": false
		},
		"format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"@weareinreach/app#lint": {
			"outputs": [".next/cache/eslint/**"]
		},
		"@weareinreach/storybook#lint": {
			"outputs": [".next/cache/eslint/**"]
		},
		"@weareinreach/web#lint": {
			"outputs": [".next/cache/eslint/**"]
		},
		"lint:fix": {
			"outputs": []
		},
		"@weareinreach/app#lint:fix": {
			"outputs": [".next/cache/eslint/**"]
		},
		"@weareinreach/storybook#lint:fix": {
			"outputs": [".next/cache/eslint/**"]
		},
		"@weareinreach/web#lint:fix": {
			"outputs": [".next/cache/eslint/**"]
		},
		"post-install": {
			"dependsOn": [],
			"outputs": ["packages/db/zod-schemas/**", "docs/database/**"]
		}
	}
}
