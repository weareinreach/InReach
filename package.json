{
	"name": "inreach",
	"version": "0.0.0",
	"private": true,
	"license": "GPL-3.0-only",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"build:aws": "turbo run build:aws",
		"cypress:open": "npx cypress open --project test",
		"cypress:run": "npx cypress run --project test",
		"cypress:run:steps": "npx cypress run --project test --env TAGS=@stepsTesting --record false",
		"cypress:run:tagged": "npx cypress run --project test --env TAGS=${TAGS}",
		"db:generate": "turbo run db:generate",
		"dev": "turbo run dev --parallel",
		"format": "turbo run format",
		"postinstall": "turbo run post-install",
		"lint": "turbo run lint",
		"lint:fix": "turbo run lint:fix",
		"lint:staged": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@changesets/cli": "2.25.2"
	},
	"devDependencies": {
		"@badeball/cypress-cucumber-preprocessor": "13.1.0",
		"@trivago/prettier-plugin-sort-imports": "3.4.0",
		"@types/prettier": "2.7.1",
		"@weareinreach/config": "*",
		"@weareinreach/eslint-config": "*",
		"cypress": "11.0.1",
		"husky": "8.0.2",
		"lint-staged": "13.0.3",
		"prettier": "2.7.1",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-packagejson": "2.3.0",
		"prettier-plugin-prisma": "4.4.0",
		"turbo": "1.6.3",
		"typescript": "4.8.4"
	},
	"peerDependencies": {
		"@weareinreach/config": "*",
		"@weareinreach/eslint-config": "*"
	},
	"packageManager": "pnpm@7.15.0",
	"engines": {
		"node": ">=16",
		"pnpm": ">=7"
	},
	"cypress-cucumber-preprocessor": {
		"stepDefinitions": "test/cypress/support/step_definitions/**/*.{js,ts}"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "^18",
				"react-dom": "^18",
				"next": "^13",
				"webpack": "^5",
				"sanity": "^3.0.0-rc.0",
				"listr2": "^5",
				"prisma": "^4"
			},
			"overrides": {
				"glob-parent@3.1.0": "glob-parent@^5.1.2",
				"trim@0.0.1": "trim@^0.0.3",
				"trim@<0.0.3": ">=0.0.3",
				"trim-newlines@<3.0.1": ">=3.0.1",
				"got@<11.8.5": ">=11.8.5",
				"undici@>=4.8.2 <=5.5.0": ">=5.5.1",
				"glob-parent@<5.1.2": ">=5.1.2",
				"node-fetch@<2.6.1": ">=2.6.1",
				"undici@<=5.8.1": ">=5.8.2",
				"undici@<5.8.0": ">=5.8.0",
				"node-fetch@<2.6.7": ">=2.6.7"
			}
		},
		"patchedDependencies": {
			"zod-prisma@0.5.4": "patches/zod-prisma@0.5.4.patch"
		},
		"overrides": {
			"trim@<0.0.3": ">=0.0.3",
			"trim-newlines@<3.0.1": ">=3.0.1",
			"got@<11.8.5": ">=11.8.5",
			"undici@>=4.8.2 <=5.5.0": ">=5.5.1",
			"glob-parent@<5.1.2": ">=5.1.2",
			"node-fetch@<2.6.1": ">=2.6.1",
			"undici@<=5.8.1": ">=5.8.2",
			"undici@<5.8.0": ">=5.8.0",
			"node-fetch@<2.6.7": ">=2.6.7"
		}
	}
}
