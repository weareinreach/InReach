{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"dynamicImport": true
		},
		"transform": {
			"react": {
				"runtime": "automatic",
				"refresh": true
			}
		},
		"loose": false,
		"externalHelpers": true,
		"keepClassNames": false
	},
	"minify": false,
	"sourceMaps": true
}
