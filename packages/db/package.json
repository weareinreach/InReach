{
	"name": "@weareinreach/db",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:logs": "pnpm run \"/^clean:logs:.*/\"",
		"clean:logs:data-migration": "rm -rf ./prisma/migration-logs/*.log",
		"clean:node": "rm -rf ./node_modules/ || true",
		"clean:zod": "rm -rf ./zod-schemas/* || true",
		"db:dataMigrate": "pnpm with-env tsx ./prisma/dataMigrationRunner.ts",
		"db:deploy": "pnpm with-env prisma migrate deploy",
		"db:generate": "pnpm clean:zod && pnpm with-env prisma generate --generator client --generator enum --no-hints && prisma -v && pnpm format:enums",
		"db:generate:aws": "pnpm with-env prisma generate --generator aws",
		"db:generate:kysely": "pnpm with-env kysely-codegen || true",
		"db:migrate": "pnpm with-env prisma migrate dev",
		"db:migrate:edit": "pnpm with-env prisma migrate dev --create-only --skip-seed",
		"db:repl": "pnpm dlx @prismatools/console@latest -c ../../node_modules/@prisma/client",
		"db:reset": "pnpm with-env prisma migrate reset",
		"db:run-seed": "echo 'No seed script defined'",
		"db:seed": "pnpm with-env prisma db seed",
		"db:snap:capture": "pnpm with-env snaplet snapshot create",
		"db:snap:restore": "pnpm with-env snaplet snapshot restore --latest",
		"db:studio": "pnpm with-env prisma studio",
		"format": "prettier --cache --cache-strategy metadata --write --ignore-unknown . && prisma format",
		"format:all": "pnpm format:export && pnpm format:generated",
		"format:enums": "prettier --write ./enums/*",
		"format:export": "prettier --cache --cache-strategy metadata --write ./data-transfer/**/*.{ts,json}",
		"format:generated": "prettier --cache --cache-strategy metadata --write ./zod*/*",
		"generate:data": "pnpm with-env tsx ./lib/generateData.ts",
		"inspect-queries": "prisma-query-inspector start",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"predev": "pnpm db:generate",
		"prisma": "pnpm with-env prisma",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@opentelemetry/exporter-trace-otlp-http": "0.52.1",
		"@opentelemetry/instrumentation": "0.52.1",
		"@opentelemetry/resources": "1.25.1",
		"@opentelemetry/sdk-trace-base": "1.25.1",
		"@opentelemetry/sdk-trace-node": "1.25.1",
		"@opentelemetry/semantic-conventions": "1.25.1",
		"@prisma/client": "5.18.0",
		"@prisma/instrumentation": "5.18.0",
		"@vercel/kv": "2.0.0",
		"@vercel/postgres-kysely": "0.9.0",
		"@weareinreach/crowdin": "workspace:*",
		"@weareinreach/env": "workspace:*",
		"@weareinreach/util": "workspace:*",
		"id128": "1.6.6",
		"json-schema-to-zod": "2.0.14",
		"kysely": "0.27.4",
		"ms": "2.1.3",
		"pg": "8.12.0",
		"prisma-kysely": "1.8.0",
		"sql-bricks": "3.0.1",
		"superjson": "2.2.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@octokit/core": "5.2.0",
		"@snaplet/copycat": "5.0.0",
		"@snaplet/seed": "0.98.0",
		"@terraformer/wkt": "2.2.1",
		"@turbo/gen": "2.0.11",
		"@turf/helpers": "7.0.0",
		"@types/eslint": "8.56.11",
		"@types/inquirer": "9.0.7",
		"@types/inquirer-autocomplete-prompt": "3.0.3",
		"@types/luxon": "3.4.2",
		"@types/ms": "0.7.34",
		"@types/node": "20.14.14",
		"@types/papaparse": "5.3.14",
		"@types/pg": "8.11.6",
		"@types/terraformer__wkt": "2.0.3",
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "workspace:*",
		"dotenv": "16.4.5",
		"dotenv-cli": "7.4.2",
		"eslint": "8.57.0",
		"fast-xml-parser": "4.4.1",
		"flat": "6.0.1",
		"google-auth-library": "9.13.0",
		"google-spreadsheet": "4.1.2",
		"googleapis": "137.1.0",
		"inquirer": "9.3.6",
		"inquirer-autocomplete-prompt": "3.0.1",
		"just-compact": "3.2.0",
		"just-group-by": "2.2.0",
		"kysely-codegen": "0.15.0",
		"libphonenumber-js": "1.11.5",
		"listr2": "8.2.4",
		"log-update": "6.1.0",
		"luxon": "3.5.0",
		"p-queue": "8.0.1",
		"p-retry": "6.2.0",
		"papaparse": "5.4.1",
		"prettier": "3.3.3",
		"pretty-bytes": "6.1.1",
		"pretty-ms": "9.1.0",
		"prisma": "5.18.0",
		"prisma-dbml-generator": "0.12.0",
		"prisma-generator-ts-enums": "1.1.0",
		"prisma-query-inspector": "1.4.4",
		"prisma-query-log": "3.2.1",
		"slugify": "1.6.6",
		"snaplet": "0.92.1",
		"social-links": "1.14.0",
		"sql-bricks-postgres": "0.6.0",
		"string-byte-length": "3.0.0",
		"tiny-invariant": "1.3.3",
		"tsx": "4.16.5",
		"type-fest": "4.23.0",
		"typescript": "5.5.4",
		"xliff": "6.2.1",
		"zod-prisma-types": "3.1.8",
		"zod-to-json-schema": "3.23.2"
	},
	"prisma": {
		"seed": "turbo run db:run-seed"
	}
}
