{
	"name": "@weareinreach/aws-user-migrate",
	"version": "0.0.0",
	"description": "Lambda to gradually migrate existing users to AWS Cognito",
	"license": "GPL-3.0-only",
	"author": "",
	"main": "dist/index.js",
	"source": "src/index.ts",
	"scripts": {
		"build:aws": "pnpm clean && pnpm bundle && pnpm package",
		"bundle": "ncc build ./src/index.ts",
		"clean": "rm -fr dist",
		"format": "prettier --write --ignore-unknown .",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"package": "cd dist && zip -r bundle.zip *"
	},
	"dependencies": {
		"@weareinreach/db": "workspace:^0.0.0",
		"aws-sdk": "^2.1246.0"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.108",
		"@types/node": "16.18.0",
		"@vercel/ncc": "^0.34.0",
		"@weareinreach/config": "workspace:^0.0.0",
		"@weareinreach/eslint-config": "workspace:^0.0.0",
		"aws-lambda": "^1.0.7",
		"esbuild": "^0.15.13",
		"eslint": "^8.26.0",
		"parcel": "^2.7.0",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	}
}
