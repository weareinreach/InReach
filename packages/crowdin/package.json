{
	"name": "@weareinreach/crowdin",
	"version": "0.100.0",
	"private": true,
	"exports": {
		"./api": "./api/index.ts",
		"./api/edge": "./api/edge.ts",
		"./cache": "./cache/index.ts",
		"./ota": "./ota/index.ts",
		"./ota/edge": "./ota/edge.ts"
	},
	"main": "./index.ts",
	"types": "./index.ts",
	"typesVersions": {
		"*": {
			"api": [
				"./api/index.ts"
			],
			"api/edge": [
				"./api/edge.ts"
			],
			"cache": [
				"./cache/index.ts"
			],
			"ota": [
				"./ota/index.ts"
			],
			"ota/edge": [
				"./ota/edge.ts"
			]
		}
	},
	"scripts": {
		"clean:node": "rm -rf ./node_modules/ || true",
		"format": "prettier --write --cache --cache-strategy metadata --ignore-unknown .",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@crowdin/crowdin-api-client": "1.28.2",
		"@crowdin/ota-client": "1.0.0",
		"@opentelemetry/api": "1.7.0",
		"@vercel/kv": "1.0.0",
		"@weareinreach/util": "workspace:*",
		"flat": "6.0.1",
		"object-sizeof": "2.6.3",
		"pretty-bytes": "6.1.1"
	},
	"devDependencies": {
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "workspace:*",
		"prettier": "3.1.0",
		"typescript": "5.3.2"
	}
}
