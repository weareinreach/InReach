{
	"name": "@weareinreach/db",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:export": "rm -rf ./datastore/v1/mongodb/{output,output-types}/* || true",
		"clean:logs": "pnpm run \"/^clean:logs:.*/\"",
		"clean:logs:data-migration": "rm -rf ./prisma/migration-logs/*.log",
		"clean:logs:seed": "rm -rf ./seed/logs/*.log",
		"clean:node": "rm -rf ./node_modules/ || true",
		"clean:zod": "rm -rf ./zod-schemas/* || true",
		"db:dataMigrate": "pnpm with-env tsx ./prisma/dataMigrationRunner.ts",
		"db:deploy": "pnpm with-env prisma migrate deploy",
		"db:generate": "pnpm clean:zod && pnpm with-env prisma generate && prisma -v",
		"db:generate:aws": "pnpm install && pnpm with-env prisma generate",
		"db:generate:proxy": "pnpm clean:zod && pnpm with-env prisma generate --data-proxy && prisma -v",
		"db:migrate": "pnpm with-env prisma migrate dev",
		"db:migrate:edit": "pnpm with-env prisma migrate dev --create-only --skip-seed",
		"db:mongoexport": "pnpm clean:export && pnpm with-env tsx ./seed/migrate-v1/mongoexport.ts",
		"db:repl": "pnpm dlx @prismatools/console@latest -c ../../node_modules/@prisma/client",
		"db:reset": "pnpm with-env prisma migrate reset",
		"db:run-seed": "tsx ./seed/starterData.ts",
		"db:seed": "pnpm with-env prisma db seed",
		"db:seed:update": "SEED_UPDATE=true pnpm with-env prisma db seed",
		"db:studio": "pnpm with-env prisma studio",
		"download:seedData": "tsx ./seed/downloadData.ts",
		"format": "prettier --cache --cache-strategy metadata --write --ignore-unknown . && prisma format",
		"format:all": "pnpm format:export && pnpm format:generated",
		"format:export": "prettier --cache --cache-strategy metadata --write ./data-transfer/**/*.{ts,json}",
		"format:generated": "prettier --cache --cache-strategy metadata --write ./zod*/*",
		"generate:data": "pnpm with-env tsx ./lib/generateData.ts",
		"inspect-queries": "prisma-query-inspector start",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"predev": "pnpm db:generate",
		"prisma": "pnpm with-env prisma",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@crowdin/crowdin-api-client": "1.23.2",
		"@opentelemetry/exporter-trace-otlp-http": "0.40.0",
		"@opentelemetry/instrumentation": "0.40.0",
		"@opentelemetry/resources": "1.14.0",
		"@opentelemetry/sdk-trace-base": "1.14.0",
		"@opentelemetry/sdk-trace-node": "1.14.0",
		"@opentelemetry/semantic-conventions": "1.14.0",
		"@prisma/client": "4.16.2",
		"@prisma/instrumentation": "4.16.2",
		"@vercel/kv": "0.2.2",
		"@vercel/postgres-kysely": "0.4.0",
		"@weareinreach/env": "workspace:*",
		"kysely": "0.25.0",
		"prisma-kysely": "1.4.2",
		"sql-bricks": "3.0.0",
		"superjson": "1.12.4",
		"tslog": "4.8.2",
		"ulid": "2.3.0",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"@faker-js/faker": "7.6.0",
		"@nuskin/mexico-state-lookup": "2.1.0",
		"@placemarkio/check-geojson": "0.1.12",
		"@terraformer/wkt": "2.2.0",
		"@turf/buffer": "6.5.0",
		"@turf/helpers": "6.5.0",
		"@turf/points-within-polygon": "6.5.0",
		"@turf/simplify": "6.5.0",
		"@types/eslint": "8.40.2",
		"@types/flat": "5.0.2",
		"@types/geojson": "7946.0.10",
		"@types/inquirer": "9.0.3",
		"@types/iso-3166-2": "1.0.0",
		"@types/langs": "2.0.1",
		"@types/luxon": "3.3.0",
		"@types/node": "18.16.18",
		"@types/papaparse": "5.3.7",
		"@types/prettier": "2.7.3",
		"@types/recursive-readdir": "2.2.1",
		"@types/terraformer__wkt": "2.0.0",
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "0.100.0",
		"@zerodep/geo.stateiso": "0.1.4",
		"axios": "1.4.0",
		"commander": "11.0.0",
		"countries-languages": "1.0.3",
		"dotenv": "16.3.1",
		"dotenv-cli": "7.2.1",
		"eslint": "8.43.0",
		"flat": "5.0.2",
		"geojson": "0.5.0",
		"inquirer": "9.2.7",
		"iso-3166-2": "1.0.0",
		"iso-google-locales": "3.0.4",
		"just-capitalize": "3.2.0",
		"just-compact": "3.2.0",
		"just-flush": "2.3.0",
		"langs": "2.0.0",
		"libphonenumber-js": "1.10.37",
		"listr2": "6.6.0",
		"luxon": "3.3.0",
		"mongoback": "3.0.6",
		"papaparse": "5.4.1",
		"prettier": "2.8.8",
		"pretty-bytes": "6.1.0",
		"prisma": "4.16.2",
		"prisma-dbml-generator": "0.10.0",
		"prisma-query-inspector": "1.4.4",
		"prisma-query-log": "3.2.0",
		"quicktype-core": "23.0.48",
		"recursive-readdir": "2.2.3",
		"shoetest": "1.2.2",
		"slugify": "1.6.6",
		"sql-bricks-postgres": "0.6.0",
		"tiny-invariant": "1.3.1",
		"tsx": "3.12.7",
		"type-fest": "3.12.0",
		"typescript": "5.1.6",
		"zod-prisma-types": "2.7.4",
		"zod-to-json-schema": "3.21.3"
	},
	"prisma": {
		"seed": "turbo run db:run-seed"
	}
}
