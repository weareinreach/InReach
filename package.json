{
  "name": "inreach",
  "version": "0.0.0",
  "private": true,
  "license": "GPL-3.0-only",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cypress:open": "npx cypress open --project test",
    "cypress:run": "npx cypress run --project test",
    "cypress:run:tagged": "npx cypress run --project test --env TAGS=${TAGS}",
    "cypress:run:steps": "npx cypress run --project test --env TAGS=@stepsTesting --record false",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.3",
    "cypress": "^10.8.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "prettier": "latest",
    "turbo": "^1.6.0",
    "typescript": "^4.8.3"
  },
  "engines": {
    "pnpm": ">=7",
    "node": ">=16"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.4"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "test/cypress/support/step_definitions/**/*.{js,ts}"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^18",
        "react-dom": "^18"
      }
    }
  },
  "packageManager": "pnpm@7.14.0"
}
