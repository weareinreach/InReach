{
	"name": "@weareinreach/util",
	"version": "0.100.0",
	"private": true,
	"sideEffects": false,
	"exports": {
		"./logger": "./logger/index.ts",
		"./superjson": "./superjson/index.ts"
	},
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:node": "rm -rf ./node_modules/ || true",
		"format": "prettier --write --cache --cache-strategy metadata --ignore-unknown .",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@vercel/edge-config": "0.4.1",
		"luxon": "3.4.3",
		"superjson": "1.13.3",
		"tslog": "4.9.2"
	},
	"devDependencies": {
		"@types/luxon": "3.3.2",
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "0.100.0"
	}
}
