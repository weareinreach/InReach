{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"env": ["NODE_ENV"],
			"outputs": ["dist/**", ".next/**", "storybook-static/**"]
		},
		"@weareinreach/app#build": {
			"dependsOn": ["^build", "post-install"],
			"env": [
				"DATABASE_URL",
				"NEXTAUTH_SECRET",
				"NEXTAUTH_URL",
				"VERCEL_URL",
				"PORT",
				"COGNITO_CLIENT_ID",
				"COGNITO_CLIENT_SECRET",
				"COGNITO_ISSUER",
				"COGNITO_USER_POOL_ID",
				"AWS_REGION"
			]
		},
		"@weareinreach/web#build": {
			"dependsOn": ["^build"],
			"env": [
				"WEB_SENTRY_DSN",
				"SANITY_TOKEN",
				"NEXT_PUBLIC_SANITY_API_VERSION",
				"NEXT_PUBLIC_SANITY_DATASET",
				"NEXT_PUBLIC_SANITY_PROJECT_ID",
				"NEXT_PUBLIC_WEB_SENTRY_DSN"
			]
		},
		"@weareinreach/db#build": {
			"dependsOn": ["clean", "db:generate"],
			"env": ["DATABASE_URL"]
		},
		"chromatic": {
			"dependsOn": ["build"],
			"env": ["CHROMATIC_PROJECT_TOKEN"]
		},
		"db:generate": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"format": {
			"outputs": []
		},
		"lint": {
			"outputs": [".next/cache/eslint/**"]
		},
		"lint:fix": {
			"outputs": [".next/cache/eslint/**"]
		},
		"post-install": {
			"cache": false
		}
	}
}
