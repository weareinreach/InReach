{
	"$schema": "https://turborepo.org/schema.json",
	"globalEnv": ["NODE_ENV"],
	"pipeline": {
		"//#clean": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"env": ["NODE_ENV"]
		},
		"build:aws": {},
		"build:icons": {},
		"chromatic": {},
		"clean": {
			"cache": false
		},
		"clean:node": {
			"cache": false
		},
		"db:deploy": {},
		"db:generate": {},
		"deploy:aws": {},
		"dev": {
			"cache": false,
			"dependsOn": ["^db:generate"],
			"persistent": true
		},
		"download:seedData": {},
		"format": {
			"dependsOn": ["^db:generate"],
			"inputs": ["**/*.{cjs,mjs,js,jsx,ts,tsx,json,prisma,html,css,scss,yml,yaml,md}"],
			"outputs": ["node_modules/.cache/prettier/.prettier-cache"]
		},
		"generate:data": {},
		"lint": {
			"inputs": ["**/*.{cjs,mjs,js,jsx,ts,tsx}"],
			"outputs": [".eslintcache"]
		},
		"lint:fix": {
			"inputs": ["**/*.{cjs,mjs,js,jsx,ts,tsx}"],
			"outputs": [".eslintcache"]
		},
		"post-install": {
			"dependsOn": ["^generate:data", "^build:icons"]
		},
		"sb:build": {},
		"type-check": {
			"cache": false
		}
	}
}
