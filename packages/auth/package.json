{
	"name": "@weareinreach/auth",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"exports": {
		".": {
			"browser": "./index.browser.ts",
			"default": "./index.ts"
		},
		"./*": {
			"default": "./*"
		},
		"./confirmAccount": {
			"default": "./lib/confirmAccount.ts"
		},
		"./createUser": {
			"default": "./lib/createUser.ts"
		},
		"./deleteAccount": {
			"default": "./lib/deleteAccount.ts"
		},
		"./forgotPassword": {
			"default": "./lib/forgotPassword.ts"
		},
		"./next-auth/session": {
			"browser": "./next-auth/session.browser.ts",
			"default": "./next-auth/session.server.ts"
		},
		"./next-auth/types": {
			"default": "./next-auth/types.ts",
			"types": "./next-auth/types.ts"
		},
		"./resetPassword": {
			"default": "./lib/resetPassword.ts"
		},
		"./server": {
			"default": "./server.ts"
		},
		"./userLogin": {
			"default": "./lib/userLogin.ts"
		},
		"./userLogout": {
			"default": "./lib/userLogout.ts"
		}
	},
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:node": "rm -rf ./node_modules/ || true",
		"format": "prettier --write --cache --cache-strategy metadata --ignore-unknown .",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@aws-sdk/client-cognito-identity-provider": "3.530.0",
		"@next-auth/prisma-adapter": "1.0.7",
		"@weareinreach/db": "workspace:*",
		"@weareinreach/env": "workspace:*",
		"@weareinreach/util": "workspace:*",
		"aws-jwt-verify": "4.0.1",
		"tiny-invariant": "1.3.3",
		"tslog": "4.9.2",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@types/eslint": "8.56.5",
		"@types/node": "20.11.26",
		"@types/react": "18.2.65",
		"@types/react-dom": "18.2.21",
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "0.100.0",
		"eslint": "8.57.0",
		"next": "14.1.3",
		"next-auth": "4.24.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "5.4.2"
	},
	"peerDependencies": {
		"next": "^14",
		"next-auth": "^4",
		"react": "^18",
		"react-dom": "^18"
	}
}
