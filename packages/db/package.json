{
	"name": "@weareinreach/db",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:logs": "pnpm run \"/^clean:logs:.*/\"",
		"clean:logs:data-migration": "rm -rf ./prisma/migration-logs/*.log",
		"clean:node": "rm -rf ./node_modules/ || true",
		"clean:zod": "rm -rf ./zod-schemas/* || true",
		"db:dataMigrate": "pnpm with-env tsx ./prisma/dataMigrationRunner.ts",
		"db:deploy": "pnpm with-env prisma migrate deploy",
		"db:generate": "pnpm clean:zod && pnpm with-env prisma generate && prisma -v",
		"db:generate:aws": "pnpm install && pnpm with-env prisma generate",
		"db:generate:kysely": "pnpm with-env kysely-codegen || true",
		"db:migrate": "pnpm with-env prisma migrate dev",
		"db:migrate:edit": "pnpm with-env prisma migrate dev --create-only --skip-seed",
		"db:repl": "pnpm dlx @prismatools/console@latest -c ../../node_modules/@prisma/client",
		"db:reset": "pnpm with-env prisma migrate reset",
		"db:run-seed": "echo 'No seed script defined'",
		"db:seed": "pnpm with-env prisma db seed",
		"db:studio": "pnpm with-env prisma studio",
		"format": "prettier --cache --cache-strategy metadata --write --ignore-unknown . && prisma format",
		"format:all": "pnpm format:export && pnpm format:generated",
		"format:export": "prettier --cache --cache-strategy metadata --write ./data-transfer/**/*.{ts,json}",
		"format:generated": "prettier --cache --cache-strategy metadata --write ./zod*/*",
		"generate:data": "pnpm with-env tsx ./lib/generateData.ts",
		"inspect-queries": "prisma-query-inspector start",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"predev": "pnpm db:generate",
		"prisma": "pnpm with-env prisma",
		"type-check": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@opentelemetry/exporter-trace-otlp-http": "0.45.1",
		"@opentelemetry/instrumentation": "0.45.1",
		"@opentelemetry/resources": "1.18.1",
		"@opentelemetry/sdk-trace-base": "1.18.1",
		"@opentelemetry/sdk-trace-node": "1.18.1",
		"@opentelemetry/semantic-conventions": "1.18.1",
		"@prisma/client": "5.6.0",
		"@prisma/instrumentation": "5.6.0",
		"@vercel/kv": "1.0.0",
		"@vercel/postgres-kysely": "0.6.0",
		"@weareinreach/crowdin": "workspace:*",
		"@weareinreach/env": "workspace:*",
		"@weareinreach/util": "workspace:*",
		"id128": "1.6.6",
		"kysely": "0.26.3",
		"pg": "8.11.3",
		"prisma-kysely": "1.7.1",
		"sql-bricks": "3.0.1",
		"superjson": "2.2.1",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@octokit/core": "5.0.2",
		"@terraformer/wkt": "2.2.0",
		"@turbo/gen": "1.10.16",
		"@turf/helpers": "6.5.0",
		"@types/eslint": "8.44.7",
		"@types/inquirer": "9.0.7",
		"@types/inquirer-autocomplete-prompt": "3.0.3",
		"@types/luxon": "3.3.5",
		"@types/node": "20.10.0",
		"@types/papaparse": "5.3.14",
		"@types/pg": "8.10.9",
		"@types/terraformer__wkt": "2.0.2",
		"@weareinreach/config": "workspace:*",
		"@weareinreach/eslint-config": "0.100.0",
		"dotenv": "16.3.1",
		"dotenv-cli": "7.3.0",
		"eslint": "8.54.0",
		"google-auth-library": "9.2.0",
		"google-spreadsheet": "4.1.1",
		"inquirer": "9.2.12",
		"inquirer-autocomplete-prompt": "3.0.1",
		"just-compact": "3.2.0",
		"kysely-codegen": "0.11.0",
		"libphonenumber-js": "1.10.51",
		"listr2": "8.0.0",
		"luxon": "3.4.4",
		"p-queue": "7.4.1",
		"p-retry": "6.1.0",
		"papaparse": "5.4.1",
		"prettier": "3.1.0",
		"pretty-bytes": "6.1.1",
		"pretty-ms": "8.0.0",
		"prisma": "5.6.0",
		"prisma-dbml-generator": "0.10.0",
		"prisma-query-inspector": "1.4.4",
		"prisma-query-log": "3.2.0",
		"slugify": "1.6.6",
		"sql-bricks-postgres": "0.6.0",
		"string-byte-length": "3.0.0",
		"tiny-invariant": "1.3.1",
		"tsx": "4.5.0",
		"type-fest": "4.8.2",
		"typescript": "5.3.2",
		"zod-prisma-types": "3.1.6",
		"zod-to-json-schema": "3.22.0"
	},
	"prisma": {
		"seed": "turbo run db:run-seed"
	}
}
