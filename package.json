{
	"name": "inreach",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"scripts": {
		"build": "turbo run build",
		"build:aws": "turbo run build:aws",
		"clean": "turbo run clean:node && pnpm clean:node",
		"clean:install": "pnpm clean && pnpm install",
		"clean:node": "rm -rf ./node_modules || true",
		"db:generate": "turbo run db:generate",
		"deploy:aws": "turbo run deploy:aws",
		"dev": "tsx ./lib/devmsg.ts",
		"dev:all": "turbo run dev",
		"dev:app": "turbo run dev --filter=app",
		"dev:ui": "turbo run dev --filter=ui",
		"dev:web": "turbo run dev --filter=web",
		"format": "turbo run format --parallel",
		"lint": "turbo run lint --parallel",
		"lint:fix": "turbo run lint:fix --parallel",
		"lint:pkg": "manypkg check",
		"lint:pkg:fix": "manypkg fix",
		"lint:staged": "lint-staged",
		"prepare": "tsx ./lib/prepare.ts",
		"type-check": "turbo type-check"
	},
	"dependencies": {
		"@changesets/cli": "2.26.0",
		"@manypkg/cli": "0.20.0",
		"@types/eslint": "8.4.10",
		"@types/node": "16.18.11",
		"@types/prettier": "2.7.2",
		"@weareinreach/config": "0.100.0",
		"@weareinreach/eslint-config": "0.100.0",
		"boxen": "7.0.1",
		"husky": "8.0.3",
		"lint-staged": "13.1.0",
		"prettier": "2.8.3",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-packagejson": "2.4.2",
		"prettier-plugin-prisma": "4.9.0",
		"tsx": "3.12.2",
		"turbo": "1.7.1",
		"typescript": "4.9.5"
	},
	"packageManager": "pnpm@7.26.2",
	"engines": {
		"node": ">=16.18.0",
		"pnpm": ">=7"
	},
	"pnpm": {
		"overrides": {
			"@floating-ui/react-dom-interactions": "npm:@floating-ui/react@^0.17.0",
			"@npmcli/move-file": "npm:@npmcli/fs@^2.0.0",
			"@storybook/react-docgen-typescript-plugin@1.0.2-canary.6.9d540b91e815f8fc2f8829189deb00553559ff63.0": "npm:react-docgen-typescript-plugin@1.0.2",
			"chokidar": ">=3.0.0",
			"glob-parent@<5.1.2": ">=5.1.2",
			"listr2@<5": ">=5.0.5",
			"micromatch": ">=4.0.0",
			"node-fetch@<2.6.7": ">=2.6.7",
			"sane": ">=5.0.0",
			"sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.14",
			"trim-newlines@<3.0.1": ">=3.0.1",
			"trim@<0.0.3": ">=0.0.3",
			"tsconfig-paths": ">=4.0.0",
			"undici@<=5.8.1": ">=5.8.2",
			"uuid": ">=9.0.0"
		},
		"patchedDependencies": {
			"eslint-plugin-node@11.1.0": "patches/eslint-plugin-node@11.1.0.patch",
			"iso-google-locales@3.0.4": "patches/iso-google-locales@3.0.4.patch"
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"@storybook/components": "^7.0.0-beta.36 || next",
				"@storybook/core-events": "^7.0.0-beta.36 || next",
				"@storybook/theming": "^7.0.0-beta.36 || next",
				"babel-loader": ">=9",
				"i18next-browser-languagedetector": ">=7",
				"i18next-http-backend": ">=2",
				"listr2": ">=5",
				"msw": "^1",
				"next": ">=13",
				"prisma": ">=4",
				"react": ">=18",
				"react-dom": ">=18",
				"react-i18next": ">=12",
				"sanity": ">=3.0.0-rc.0",
				"webpack": ">=5"
			}
		}
	}
}
