{
	"name": "@weareinreach/db",
	"version": "0.100.0",
	"private": true,
	"license": "GPL-3.0-only",
	"main": "./index.ts",
	"types": "./index.ts",
	"scripts": {
		"clean:node": "rm -rf ./node_modules/ || true",
		"db:deploy": "pnpm with-env prisma migrate deploy",
		"db:generate": "pnpm with-env prisma generate && pnpm format:generated && prisma -v",
		"db:generate:aws": "pnpm install && pnpm with-env prisma generate",
		"db:migrate": "pnpm with-env prisma migrate dev",
		"db:migrate:edit": "pnpm with-env prisma migrate dev --create-only --skip-seed",
		"db:mongoexport": "pnpm with-env tsx ./data-transfer/scripts/mongoexport.ts",
		"db:seed": "pnpm with-env prisma db seed",
		"db:studio": "pnpm with-env prisma studio",
		"format": "prettier --cache --cache-strategy metadata --write --ignore-unknown . && prisma format",
		"format:all": "pnpm format:export && pnpm format:generated",
		"format:export": "prettier --cache --cache-strategy metadata --write ./data-transfer/**/*.{ts,json}",
		"format:generated": "prettier --cache --cache-strategy metadata --write ./zod*/*",
		"inspect-queries": "prisma-query-inspector start",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint --cache .",
		"lint:fix": "eslint --cache --fix .",
		"post-install": "pnpm db:generate",
		"predev": "pnpm db:generate",
		"prisma": "pnpm with-env prisma",
		"schema:clean": "rm -rf ./zod-schemas/* || true",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@prisma/client": "4.6.1",
		"prisma-docs-generator": "0.5.0",
		"prisma-query-inspector": "1.4.4",
		"zod": "3.19.1"
	},
	"devDependencies": {
		"@faker-js/faker": "7.6.0",
		"@types/node": "16.18.3",
		"@types/recursive-readdir": "2.2.1",
		"@typescript-eslint/eslint-plugin": "5.44.0",
		"@weareinreach/config": "0.100.0",
		"@weareinreach/eslint-config": "0.100.0",
		"axios": "1.1.3",
		"commander": "9.4.1",
		"dotenv": "16.0.3",
		"dotenv-cli": "6.0.0",
		"eslint": "8.28.0",
		"eslint-import-resolver-typescript": "3.5.2",
		"eslint-plugin-codegen": "0.16.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-simple-import-sort": "8.0.0",
		"eslint-plugin-turbo": "0.0.4",
		"inquirer": "9.1.4",
		"listr2": "5.0.5",
		"mongoback": "3.0.3",
		"prisma": "4.6.1",
		"quicktype-core": "6.1.0",
		"recursive-readdir": "2.2.3",
		"slugify": "1.6.5",
		"tsup": "6.5.0",
		"tsx": "3.12.1",
		"typescript": "4.9.3",
		"zod-prisma": "0.5.4"
	},
	"prisma": {
		"seed": "tsx ./seed/starter/index.ts"
	}
}
