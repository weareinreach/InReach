{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build",
				"$SENTRY_DSN",
				"$NEXT_PUBLIC_SENTRY_DSN",
				"$NODE_ENV",
				"$NEXT_PUBLIC_SANITY_API_VERSION",
				"$NEXT_PUBLIC_SANITY_DATASET",
				"$NEXT_PUBLIC_SANITY_PROJECT_ID"
			],
			"outputs": ["dist/**", ".next/**"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	}
}
