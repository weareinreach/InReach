{
	"configurations": [
		{
			"disableNetworkCache": true,
			"name": "Launch Chromium",
			"port": 9222,
			"request": "launch",
			"runtimeExecutable": "/Applications/Chromium.app/Contents/MacOS/Chromium",
			"type": "chrome",
			"url": "http://localhost:3000",
			"userDataDir": false
		},
		{
			"name": "Attach to Arc",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}",
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!${workspaceFolder:âœ¨ InReach (root)}node_modules/**"
			],
			"smartStep": true
		}
	]
}
